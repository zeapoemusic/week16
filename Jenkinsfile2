pipeline {
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages{
    stage('sonareqube scan'){
            steps{
                withSonarQubeEvn('sonarserver'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=zeapoemusic_week16'
            }
            }
            }
        stage('maven commands'){
            steps{
                sh 'mvn clean compile install package'
            }
        }
        }
}